config_version=4

_global_script_classes=[ {
"base": "TextureRect",
"class": "Card",
"language": "GDScript",
"path": "res://scenes/card/card.gd"
}, {
"base": "Tween",
"class": "CardTween",
"language": "GDScript",
"path": "res://scenes/tweening/card_tween.gd"
} ]
_global_script_class_icons={
"Card": "",
"CardTween": ""
}

[application]

config/name="Card-flipping"
run/main_scene="res://scenes/tmp/tmp.tscn"
